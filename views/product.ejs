<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Login Page</title>
    <script src="https://cdn.tailwindcss.com"></script>
    <link rel="stylesheet" href="https://unpkg.com/swiper/swiper-bundle.min.css" />
    <style>
        .swiper {
            width: 100%;
            height: 100%;
        }
    </style>
</head>

<body class="bg-white dark:bg-gray-900 text-black dark:text-white min-h-screen">

    <nav class="bg-blue-600 dark:bg-gray-800 p-4">
        <div class="container mx-auto flex justify-between items-center">
            <a class="text-white text-lg font-bold" href="#">ZakMart</a>
            <button class="lg:hidden text-white" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav"
                aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon">☰</span>
            </button>
            <div class="hidden lg:flex" id="navbarNav">
                <ul class="flex space-x-4">
                    <li>
                        <a href="/products"><button class="btn bg-yellow-400 hover:bg-black hover:text-white text-black py-2 px-4 rounded">Home</button></a>
                    </li>

                    <% if(user){ %>
                        <li>
                            <a href="/profile"><button class="btn bg-yellow-400 hover:bg-black hover:text-white text-black py-2 px-4 rounded">Profile</button></a>
                        </li>
                        <li>
                            <a href="/cart"><button class="btn bg-yellow-400 hover:bg-black hover:text-white text-black py-2 px-4 rounded">Cart</button></a>
                        </li>                            
                        <li>
                            <a href="/logout"><button class="btn bg-yellow-400 hover:bg-black hover:text-white text-black py-2 px-4 rounded">Log Out</button></a>
                        </li>
                    <% } else{ %>
                        <li>
                            <a href="/"><button class="btn bg-yellow-400 hover:bg-black hover:text-white text-black py-2 px-4 rounded">Login</button></a>
                        </li>
                        <li>
                            <a href="/signup"><button class="btn bg-yellow-400 hover:bg-black hover:text-white text-black py-2 px-4 rounded">Sign Up</button></a>
                        </li>
                    <% } %>

                </ul>
            </div>
        </div>
    </nav>

    <div class="bg-white dark:bg-gray-900 min-h-screen">
        <div class="container mx-auto">
            <div class="text-center my-8">
                <h3 class="text-3xl font-bold">Electronic Product</h3>
            </div>
            <div class="swiper">
                <div class="swiper-wrapper">
                    <div class="swiper-slide">
                        <div class="col-item border-2 border-blue-800 dark:border-gray-400 rounded-lg bg-white dark:bg-gray-800">
                            <div class="info p-4">
                                <div class="flex justify-between">
                                    <div class="price">
                                        <h5 class="text-lg font-semibold">Nvedia GPU</h5>
                                        <h5 class="text-green-600 dark:text-green-400">₹30000</h5>
                                    </div>
                                    <div class="rating flex">
                                        <i class="text-blue-800 dark:text-blue-400 fa fa-star"></i>
                                        <i class="text-blue-800 dark:text-blue-400 fa fa-star"></i>
                                    </div>
                                </div>
                            </div>
                            <div class="photo p-4">
                                <img class="py-2" src="https://m.media-amazon.com/images/I/71UStULnUyS._AC_UF1000,1000_QL80_.jpg"
                                    class="mx-auto" alt="Nvedia GPU" />
                            </div>
                            <div class="info p-4">
                                <form method="POST" action="/additem">
                                    <div class="flex flex-col space-y-2">
                                        <input style="color: black;" name="quantity" placeholder="Quantity" type="number"
                                            class="form-control border p-2 rounded">
                                        <input name="name" value="Nvedia GPU" type="hidden">
                                        <input name="price" value="30000" type="hidden">
                                        <input name="prod_id" value="P0001" type="hidden">
                                        <button class="bg-blue-600 dark:bg-gray-700 hover:bg-black hover:text-white text-white py-2 px-4 rounded">Add to Cart</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                    <div class="swiper-slide">
                        <div class="col-item border-2 border-blue-800 dark:border-gray-400 rounded-lg bg-white dark:bg-gray-800">
                            <div class="info p-4">
                                <div class="flex justify-between">
                                    <div class="price">
                                        <h5 class="text-lg font-semibold">Gaming Keyboard</h5>
                                        <h5 class="text-green-600 dark:text-green-400">₹2000</h5>
                                    </div>
                                    <div class="rating flex">
                                        <i class="text-blue-800 dark:text-blue-400 fa fa-star"></i>
                                        <i class="text-blue-800 dark:text-blue-400 fa fa-star"></i>
                                        <i class="text-blue-800 dark:text-blue-400 fa fa-star"></i>
                                    </div>
                                </div>
                            </div>
                            <div class="photo p-4">
                                <img class="py-5" src="https://rukminim2.flixcart.com/image/850/1000/xif0q/keyboard/desktop-keyboard/w/l/6/gaming-keyboard-with-87-keys-rgb-backlit-with-suspension-keys-original-imagzcgwtrabgjna.jpeg?q=20&crop=false"
                                    class="mx-auto" alt="Gaming Keyboard" />
                            </div>
                            <div class="info p-4">
                                <form method="POST" action="/additem">
                                    <div class="flex flex-col space-y-2">
                                        <input style="color: black;" name="quantity" placeholder="Quantity" type="number"
                                            class="form-control border p-2 rounded">
                                        <input name="name" value="Gaming Keyboard" type="hidden">
                                        <input name="price" value="2000" type="hidden">
                                        <input name="prod_id" value="P0002" type="hidden">
                                        <button class="bg-blue-600 dark:bg-gray-700 hover:bg-black hover:text-white text-white py-2 px-4 rounded">Add to Cart</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                    <div class="swiper-slide">
                        <div class="col-item border-2 border-blue-800 dark:border-gray-400 rounded-lg bg-white dark:bg-gray-800">
                            <div class="info p-4">
                                <div class="flex justify-between">
                                    <div class="price">
                                        <h5 class="text-lg font-semibold">Intel CPU</h5>
                                        <h5 class="text-green-600 dark:text-green-400">₹25000</h5>
                                    </div>
                                    <div class="rating flex">
                                        <i class="text-blue-800 dark:text-blue-400 fa fa-star"></i>
                                        <i class="text-blue-800 dark:text-blue-400 fa fa-star"></i>
                                    </div>
                                </div>
                            </div>
                            <div class="photo p-4">
                                <img src="https://computechstore.in/wp-content/uploads/2023/10/Intel-Core-i9-14900K-14th-Gen-24-Core-8P16E-LGA-1700-Desktop-Processor-1-1.jpg"
                                    class="mx-auto" alt="Intel CPU" />
                            </div>
                            <div class="info p-4">
                                <form method="POST" action="/additem">
                                    <div class="flex flex-col space-y-2">
                                        <input style="color: black;" name="quantity" placeholder="Quantity" type="number"
                                            class="form-control border p-2 rounded">
                                        <input name="name" value="Intel CPU" type="hidden">
                                        <input name="price" value="25000" type="hidden">
                                        <input name="prod_id" value="P0003" type="hidden">
                                        <button class="bg-blue-600 dark:bg-gray-700 hover:bg-black hover:text-white text-white py-2 px-4 rounded">Add to Cart</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                    <div class="swiper-slide">
                        <div class="col-item border-2 border-blue-800 dark:border-gray-400 rounded-lg bg-white dark:bg-gray-800">
                            <div class="info p-4">
                                <div class="flex justify-between">
                                    <div class="price">
                                        <h5 class="text-lg font-semibold">Ryzen CPU</h5>
                                        <h5 class="text-green-600 dark:text-green-400">₹23000</h5>
                                    </div>
                                    <div class="rating flex">
                                        <i class="text-blue-800 dark:text-blue-400 fa fa-star"></i>
                                        <i class="text-blue-800 dark:text-blue-400 fa fa-star"></i>
                                    </div>
                                </div>
                            </div>
                            <div class="photo p-4">
                                <img src="data:image/jpeg;base64,"
                                    class="mx-auto py-2" alt="Ryzen CPU" />
                            </div>
                            <div class="info p-4">
                                <form method="POST" action="/additem">
                                    <div class="flex flex-col space-y-2">
                                        <input style="color: black;" name="quantity" placeholder="Quantity" type="number"
                                            class="form-control border p-2 rounded">
                                        <input name="name" value="Ryzen CPU" type="hidden">
                                        <input name="price" value="23000" type="hidden">
                                        <input name="prod_id" value="P0005" type="hidden">
                                        <button class="bg-blue-600 dark:bg-gray-700 hover:bg-black hover:text-white text-white py-2 px-4 rounded">Add to Cart</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                    <div class="swiper-slide">
                        <div class="col-item border-2 border-blue-800 dark:border-gray-400 rounded-lg bg-white dark:bg-gray-800">
                            <div class="info p-4">
                                <div class="flex justify-between">
                                    <div class="price">
                                        <h5 class="text-lg font-semibold">Gaming Mouse</h5>
                                        <h5 class="text-green-600 dark:text-green-400">₹1000</h5>
                                    </div>
                                    <div class="rating flex">
                                        <i class="text-blue-800 dark:text-blue-400 fa fa-star"></i>
                                        <i class="text-blue-800 dark:text-blue-400 fa fa-star"></i>
                                        <i class="text-blue-800 dark:text-blue-400 fa fa-star"></i>
                                        <i class="text-blue-800 dark:text-blue-400 fa fa-star"></i>
                                    </div>
                                </div>
                            </div>
                            <div class="photo p-4">
                                <img src="https://m.media-amazon.com/images/I/61qN9d08hgL.jpg" class="mx-auto"
                                    alt="Gaming Mouse" />
                            </div>
                            <div class="info p-4">
                                <form method="POST" action="/additem">
                                    <div class="flex flex-col space-y-2">
                                        <input style="color: black;" name="quantity" placeholder="Quantity" type="number"
                                            class="form-control border p-2 rounded">
                                        <input name="name" value="Gaming Mouse" type="hidden">
                                        <input name="price" value="1000" type="hidden">
                                        <input name="prod_id" value="P0004" type="hidden">
                                        <button class="bg-blue-600 dark:bg-gray-700 hover:bg-black hover:text-white text-white py-2 px-4 rounded">Add to Cart</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Add Pagination -->
                <!-- <div class="swiper-pagination"></div> -->
                <!-- Add Navigation -->
                <div class="swiper-button-next"></div>
                <div class="swiper-button-prev"></div>
            </div>
        </div>
    </div>

    <script src="https://unpkg.com/swiper/swiper-bundle.min.js"></script>
    <script>
        var swiper = new Swiper('.swiper', {
            slidesPerView: 1,
            spaceBetween: 10,
            loop: true,
            pagination: {
                el: '.swiper-pagination',
                clickable: true,
            },
            navigation: {
                nextEl: '.swiper-button-next',
                prevEl: '.swiper-button-prev',
            },
            breakpoints: {
                640: {
                    slidesPerView: 1,
                    spaceBetween: 20,
                },
                768: {
                    slidesPerView: 2,
                    spaceBetween: 40,
                },
                1024: {
                    slidesPerView: 4,
                    spaceBetween: 50,
                },
            },
        });
    </script>
</body>

</html>
